+  is(Object.keys(root.calls.A.calls.E.calls.F.calls).length, 0,
+    "The correct number of child calls were calculated for the '.A.E.F' node.");
+
+  // Insert new nodes in the tree.
+
+  root.insert({
+    time: 20,
+    frames: [
+      { location: "(root)" },
+      { location: "A" },
+      { location: "B" },
+      { location: "C" },
+      { location: "D" },
+      { location: "E" },
+      { location: "F" },
+      { location: "G" }
